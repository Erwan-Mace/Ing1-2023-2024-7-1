int score; // on définit la variable score

score=timer*100

if (f == NombreOiseaux && timer>0){ // si tous les oiseaux ont étaient attrapés et que le timer est superieur à 0 alors le joueur a réussi le niveau
        printf("Bravo! Vous avez attrape tous les oiseaux! Votre score est de %d",score);
    }
    else  {
        printf("Time's up, Vous avez perdu...\n"); // sinon si le joueur n'a pas attrapé tous les oiseaux et/ou le timer est inferieur à 0 alors le joueur n'a pas validé le niveau
    }
